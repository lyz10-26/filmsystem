\n          this.tableData=res.data\r\n          this.$message({\r\n            type: \"success\",\r\n            message: res.msg,\r\n          });\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg,\r\n          });\r\n        }\r\n      })\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\nh1 {\r\n  text-align: center;\r\n  margin-top: 20px;;\r\n}\r\n\r\n</style>"]}]}