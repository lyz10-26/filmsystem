影推荐\r\n        </el-menu-item>\r\n      </el-sub-menu>\r\n      <el-menu-item index=\"4\" disabled>\r\n        <i class=\"el-icon-shopping-cart-2\"></i>\r\n        <span>购票系统</span>\r\n      </el-menu-item>\r\n      <el-sub-menu index=\"5\">\r\n        <template #title>\r\n          <i class=\"el-icon-setting\"></i>\r\n          <span>后台管理</span>\r\n        </template>\r\n        <el-menu-item\r\n          index=\"/admin\"\r\n          class=\"el-icon-video-camera-solid\"\r\n          @click=\"pushAdmin\"\r\n          >影片管理\r\n        </el-menu-item>\r\n        <el-menu-item index=\"/admin1\" class=\"el-icon-user\" @click=\"pushAdmin1\"\r\n          >用户管理\r\n        </el-menu-item>\r\n      </el-sub-menu>\r\n    </el-menu>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Aside\",\r\n  data() {\r\n    return {\r\n      filmInfo: {},\r\n      user: {},\r\n      path: this.$route.path\r\n    };\r\n  },\r\n  mounted() {\r\n    this.user = JSON.parse(JSON.parse(sessionStorage.getItem(\"user\")));\r\n  },\r\n  methods: {\r\n    pushAir() {\r\n      this.$router.push({\r\n        path: \"/air\",\r\n      });\r\n    },\r\n    pushHome() {\r\n      this.$router.push({\r\n        path: \"/home\",\r\n      });\r\n    },\r\n    pushComing() {\r\n      this.$router.push({\r\n        path: \"/coming\",\r\n      });\r\n    },\r\n    pushUserRec() {\r\n      this.$router.push({\r\n        path: \"/userRecommend\",\r\n      });\r\n    },\r\n    pushFilmRec() {\r\n      this.$router.push({\r\n        path: \"/filmRecommend\",\r\n      });\r\n    },\r\n    pushAdmin() {\r\n      if (this.user.role == \"超级管理员\") {\r\n        this.$router.push({\r\n          path: \"/admin\",\r\n        });\r\n      } else {\r\n        this.$message({\r\n          type: \"error\",\r\n          message: \"没有权限！\",\r\n        });\r\n      }\r\n    },\r\n    pushAdmin1() {\r\n      if (this.user.role == \"超级管理员\") {\r\n        this.$router.push({\r\n          path: \"/admin1\",\r\n        });\r\n      } else {\r\n        this.$message({\r\n          type: \"error\",\r\n          message: \"没有权限！\",\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}