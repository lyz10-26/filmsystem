              allow-half\r\n                      @change=\"pop\"\r\n                  >\r\n                  </el-rate>\r\n                  <template #reference>\r\n                    <el-button round type=\"success\">评分</el-button>\r\n                  </template>\r\n                </el-popover>\r\n              </div>\r\n            </li>\r\n            <li class=\"bt_classify_name_empty\" style=\"visibility: hidden\"></li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n      <div\r\n          style=\"\r\n          width: 600px;\r\n          margin-top: 20px;\r\n          margin-bottom: 10px;\r\n          font-size: 160%;\r\n          color: rgb(234, 78, 75);\r\n        \"\r\n      >\r\n        <h2>今日票房</h2>\r\n        <div style=\"display: flex; margin-top: 25px;\">\r\n          <img src=\"../assets/img/流浪地球2.jpg\" alt=\"\" style=\"width: 90px\"/>\r\n          <div>\r\n            <p\r\n                style=\"\r\n                font-size: 80%;\r\n                color: black;\r\n                margin-left: 15px;\r\n                margin-top: 20px;\r\n                \r\n              \"\r\n            >\r\n              流浪地球2\r\n            </p>\r\n            <p style=\"font-size: 60%; margin-left: 15px; margin-top: 40px\">\r\n              43亿5892万\r\n            </p>\r\n          </div>\r\n        </div>\r\n        <div style=\"margin-top: 30px;\">\r\n          <span style=\"font-style: italic\">2</span>\r\n          <span\r\n              style=\"font-size:80%; color:black; margin-left:15px; \"\r\n          >长津湖</span\r\n          >\r\n          <span style=\"font-size: 60%; margin-left: 45px\">6235.20万</span>\r\n        </div>\r\n        <div style=\"margin-top: 30px\">\r\n          <span style=\"font-style: italic\">3</span>\r\n          <span\r\n              style=\"font-size:80%; color:black; margin-left:15px; \"\r\n          >战狼2</span\r\n          >\r\n          <span style=\"font-size: 60%; margin-left: 53px\">3798.42万</span>\r\n        </div>\r\n        <div style=\"margin-top: 30px\">\r\n          <span style=\"color:rgb(153,153,153); font-style: italic\">4</span>\r\n          <span\r\n              style=\"font-size:80%; color:black; margin-left:15px; \"\r\n          >小王子</span\r\n          >\r\n          <span style=\"font-size: 60%; margin-left: 45px\">1941.11万</span>\r\n        </div>\r\n        <div style=\"margin-top: 30px\">\r\n          <span style=\"color:rgb(153,153,153); font-style: italic\">5</span>\r\n          <span\r\n              style=\"font-size:80%; color:black; margin-left:15px; \"\r\n          >绿皮书</span\r\n          >\r\n          <span style=\"font-size: 60%; margin-left: 45px\">1175.60万</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {ElMessage} from \"element-plus\";\r\n\r\nexport default {\r\n  name: \"Air\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      list: [\r\n\r\n        {name: \"流浪地球2\"},\r\n        {name: \"铃芽之旅\"},\r\n        {name: \"银河护卫队3\"},\r\n        {name: \"长空之王\"},\r\n        {name: \"人生路不熟\"},\r\n        {name: \"你的名字。\"},\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    pop() {\r\n      ElMessage({\r\n        showClose: true,\r\n        message: \"评价成功！感谢您的参与！\",\r\n        type: \"success\",\r\n      });\r\n    },\r\n    pushShop(item){\r\n      this.$router.push({\r\n        name:'Shop',\r\n        params:{\r\n          name:item.name\r\n        }\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nul {\r\n  margin: 0;\r\n  list-style: none;\r\n  padding: 1rem;\r\n  box-sizing: border-box;\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n  justify-content: space-between;\r\n}\r\n\r\nli {\r\n  flex: 0 0 33%;\r\n  height: 6rem;\r\n  display: inline-block;\r\n  /* background: rgb(240, 240, 240); */\r\n  margin-bottom: 15rem;\r\n  text-align: center;\r\n}\r\n\r\nli:hover p {\r\n  opacity: 0.7;\r\n  color: red;\r\n}\r\n\r\n.bt_classify_name_empty {\r\n  width: 0;\r\n  height: 0;\r\n}\r\n\r\n\r\n</style>"],"sourceRoot":""}]}